<UserControl x:Class="MazeMVVM.ViewLib.Controls.MazeDisplayer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MazeMVVM.ViewLib.Controls"
             
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Loaded="MazeDisplayer_OnLoaded" KeyDown="MazeDisplayer_OnKeyDown" PreviewKeyDown="MazeDisplayer_OnKeyDown">
    <Viewbox  HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Grid x:Name="grid" Height="400" Width="400" ShowGridLines="True" HorizontalAlignment="Center" VerticalAlignment="Center">
        </Grid>
    </Viewbox>
</UserControl>
