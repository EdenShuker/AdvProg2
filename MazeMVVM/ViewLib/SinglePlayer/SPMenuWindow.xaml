<Window x:Class="MazeMVVM.ViewLib.SinglePlayer.SPMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeMVVM.ViewLib"
        xmlns:controls="clr-namespace:MazeMVVM.ViewLib.Controls"
        mc:Ignorable="d"
        Title="SPMenuWindow" Height="300" Width="300"
        Closing="SPMenuWindow_OnClosing">
    <Canvas>
        <controls:GameStartMenu x:Name="StartMenu" HorizontalAlignment="Center" VerticalAlignment="Center"
                                Canvas.Top="60" Canvas.Left="10" />
        <Image Width="150" Height="44" Canvas.Left="65" Canvas.Top="15" Source="../../mazeLogo.png" />
    </Canvas>
</Window>